{
    "name": "canvas-constructor",
    "version": "3.2.0",
    "description": "A ES6 class for node-canvas with built-in functions and chained methods.",
    "main": "src/index",
    "types": "typings/index.d.ts",
    "scripts": {
        "prepublishOnly": "yarn run build:browser",
        "build:browser": "webpack",
        "lint": "eslint --ext js,ts src typings --fix",
        "test:lint": "eslint --ext js,ts src typings",
        "docs": "dg --source src --custom guides/.docconfig.json --output docs/docs.json --logging"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kyranet/canvasConstructor.git"
    },
    "unpkg": "./webpack/canvasconstructor.min.js",
    "peerDependencies": {
        "canvas": "*"
    },
    "devDependencies": {
        "@types/node": "^13.11.1",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "docgen": "github:dirigeants/docsgen",
        "eslint": "^6.8.0",
        "eslint-config-bamboo": "^3.0.0",
        "terser-webpack-plugin": "^2.3.5",
        "typescript": "^3.8.3",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11"
    },
    "keywords": [
        "node-canvas",
        "es6",
        "constructor"
    ],
    "contributors": [
        "kyranet",
        "yorkaargh"
    ],
    "author": "kyranet",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kyranet/canvasConstructor/issues"
    },
    "browser": {
        "canvas": false,
        "canvas-prebuilt": false
    },
    "engines": {
        "node": ">=8.6.0"
    },
    "homepage": "https://github.com/kyranet/canvasConstructor#readme"
}
